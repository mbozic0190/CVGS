@model IEnumerable<Conestoga_Virtual_Game_Store.Models._event>

@{
    ViewBag.Title = "Index";
}

<h2 align="center">Event List Report</h2>

<table class="table">
    <tr>
        <th>
            Event Name
        </th>
        <th>
            Event Date
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.event_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.event_date)
        </td>
    </tr>
}

</table>
