@model IEnumerable<Conestoga_Virtual_Game_Store.Models.wishlist>

@{
    ViewBag.Title = "Index";
}

<h2 align="center">Wishlist Report</h2>

<h4>Username:</h4>
<table class="table">
    <tr>
        <th>
            Game:
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.game.game_name)
        </td>
    </tr>
}

</table>
